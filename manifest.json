{
    "manifest_version": 2,

    "name": "Jira Timer",
    "description": "Track Time direct to JIRA.",
    "version": "1.0",

    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}